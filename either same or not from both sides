class Main {
    public static void main(String[] args) {
        String num="abb";
        String ans="";
        for(int i=num.length()-1;i>=0;i--){
            ans+=num.charAt(i);
        }
        if(ans.equals(num)){
            System.out.println("yes");
        }
        else{
            System.out.println("No");
        }
    }    
}
