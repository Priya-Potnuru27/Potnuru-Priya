stones="aaAAAAbbb"
jewels="aaA"
ans=0
for i in range(len(stones)):
    for j in range(len(jewels)):
        if i==j:
            ans += 1
            break
print(ans)

#another way

stones="aabbcd"
jewels="abba"
ans=0
for i in stones:
    if i in jewels:
        ans += 1
        
        
print(ans)
